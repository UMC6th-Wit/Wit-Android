<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FEF9E8">
    <TextView
        android:id="@+id/mypage_tv1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MY"
        android:fontFamily="@font/suitbold"
        android:textSize="18sp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:textColor="@color/black"/>
    <ImageView
        android:layout_width="24dp"
        android:layout_height="24dp"
        app:layout_constraintTop_toTopOf="@id/mypage_tv1"
        app:layout_constraintBottom_toBottomOf="@id/mypage_tv1"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="18dp"
        android:src="@drawable/mypage_back"/>




</androidx.constraintlayout.widget.ConstraintLayout>